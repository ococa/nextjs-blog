{
  "name": "nextjs-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "ss": "concurrently \"npm run typrorm:build\" \"npm run dev\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typrorm:build": "npx babel -w ./src --out-dir dist --extensions \".ts,.tsx\"",
    "m:create": "typeorm migration:create",
    "m:run": "typeorm migration:run",
    "m:revert": "typeorm migration:revert",
    "entity:create": "typeorm entity:create"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "next": "10.0.8",
    "pg": "^8.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.31",
    "typescript": "^4.2.3",
    "ua-parser-js": "^0.7.24"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@types/node": "^14.14.32",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/ua-parser-js": "^0.7.35",
    "concurrently": "^6.0.0"
  }
}
